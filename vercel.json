{
  "version": 2,
  "builds": [
    {
      "src": "angularapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/angularapp"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL": "https://your-api-app.railway.app"
  }
}
